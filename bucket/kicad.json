    "checkver": {
        "url": "https://kicad.org/download/windows/",
        "regex": "kicad-([\\d._]+)-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://kicad-downloads.s3.cern.ch/windows/stable/kicad-$version-x86_64.exe#/dl.7z"
